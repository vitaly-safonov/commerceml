{
    "name": "vitalySafonov/commerceml",
    "type": "library",
    "description": "Library for CommerceML 2.0 parser",
    "keywords": ["parser", "commerceml", "xml"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Koretskiy",
            "email": "gillbeits@gmail.com"
        }
    ],
    "require": {
        "ext-simplexml": "*",
        "ext-xmlreader": "*",
        "php": ">=5.6",
        "symfony/event-dispatcher": ">=2.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "CommerceMLParser": "src/"
        }
    },
    "require-dev": {
        "codeception/codeception": "4.1",
        "codeception/module-asserts": "dev-master",
        "symfony/var-dumper": "^5.2@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-alpha-2": "2.0.x-dev"
        }
    }
}
